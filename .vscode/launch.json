{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flappy Bird Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "/home/kovitang/Документы/flappy_bird_on_C/main",
            "cwd": "${workspaceFolder}",
            "args": [
                "/home/kovitang/Документы/flappy_bird_on_C/main.c",
                "-o", "/home/kovitang/Документы/flappy_bird_on_C/main",
                "-I/usr/include/SDL2",
                "-D_REENTRANT",
                "-lSDL2",
                "-lSDL2_image",
                "-lSDL2_ttf"
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build Flappy Bird"
        }
    ]
}
